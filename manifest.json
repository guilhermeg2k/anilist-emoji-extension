{
  "manifest_version": 3,
  "name": "Anilist Emoji Extension",
  "description": "Anilist Emoji Extension",
  "version": "1.0",
  "permissions": [
    "storage"
  ],
  "content_scripts": [
    {
      "js": [
        "src/index.js"
      ],
      "css": [
        "src/index.css"
      ],
      "matches": [
        "https://anilist.co/*"
      ],
      "run_at": "document_end"
    }
  ]
}